@{
    ViewData["Title"] = "News";
}
@using Univibe_Web.Models

@for (int i = 0; i < 3 && i < (ViewBag.Rss as List<RssModel>).Count; ++i)
{
    <div>
        <h1><a href=@ViewBag.Rss[i].Link>@ViewBag.Rss[i].Title</a></h1>
        <div>
            @Html.Raw(@ViewBag.Rss[i].Description)
        </div>
    </div>

}